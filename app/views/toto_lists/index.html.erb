<p id="notice"><%= notice %></p>

<h1>Toto Lists</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @toto_lists.each do |toto_list| %>
      <tr>
        <td><%= toto_list.title %></td>
        <td><%= toto_list.description %></td>
        <td><%= link_to 'Show', toto_list %></td>
        <td><%= link_to 'Edit', edit_toto_list_path(toto_list) %></td>
        <td><%= link_to 'Destroy', toto_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Toto List', new_toto_list_path %>
